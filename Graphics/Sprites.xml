    </PlayerInteract>
	
	<BlackHole path="AletrisSandbox/ModifiedBlackHole/" start="Blackhole"> 
		<Origin x="-0.5" y="-0.5"/>
		<Loop id="Blackhole" path="Blackhole" delay="0.1"/>
    </BlackHole>
	
	<BHAura path="AletrisSandbox/ModifiedBlackHole/" start="Aura"> 
