<?xml version="1.0" encoding="utf-8" ?>
<Sprites>

    <NeedleEntity path="danger/AletrisSandbox/NeedleEntity/" start="idle">
		<Justify x="0.5" y="0.5"/>
        <Loop id="idle" path="needle" delay="0.1"/>
    </NeedleEntity>

    <MiniNeedleEntity path="danger/AletrisSandbox/NeedleEntity/" start="idle">
		<Justify x="0.5" y="0.5"/>
        <Loop id="idle" path="mini" delay="0.1"/>
