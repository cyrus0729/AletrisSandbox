    </HitboxController>
	
	<IWBTGun path="AletrisSandbox/gun/" start="-"> 
		<Loop id="-" path="gun" delay="0.1"/>
    </IWBTGun>
	
	<IWBTBullet path="AletrisSandbox/gun/" start="-"> 
		<Loop id="-" path="bullet" delay="0.1"/>
    </IWBTBullet>

	<IWBTCross path="AletrisSandbox/gun/" start="-"> 
		<Loop id="-" path="crosshair" delay="0.1"/>
    </IWBTCross>
	
	<BulletRedirector path="objects/AletrisSandbox/BulletRedirector/" start="idle"> 
		<Center/>
		<Loop id="idle" path="idle" delay="0"/>
